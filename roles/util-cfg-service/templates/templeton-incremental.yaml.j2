amqp:
  uri: {{amqp_de_uri}}
  exchange:
    name: {{amqp.de.name}}
    type: {{amqp.de.type}}
  queue_prefix: {{ templeton.amqp.queue_prefix }}

elasticsearch:
  base: {{ elasticsearch_uri }}
  username: {{ indices.data.es_user }}
  password: {{ indices.data.es_password }}
  index: {{ indices.data.write_alias }}

db:
  uri: postgres://{{ metadata_db_user }}:{{ metadata_db_password }}@{{ metadata_db_host }}:{{ metadata_db_port }}/{{ metadata_db_name }}?sslmode=disable
