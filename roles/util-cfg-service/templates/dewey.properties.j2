dewey.environment-name          = {{ environment_name }}

dewey.amqp.uri                  = amqp://{{ irods.amqp_broker.user }}:{{ irods.amqp_broker.password }}@{{ irods.amqp_broker.host }}:{{ irods.amqp_broker.port }}/%2F
dewey.amqp.exchange.name        = {{ amqp_irods_exchange }}
dewey.amqp.exchange.durable     = {{ amqp_irods_exchange_durable }}
dewey.amqp.exchange.auto-delete = {{ amqp_irods_exchange_auto_delete }}
dewey.amqp.qos                  = 100

dewey.events.amqp.uri           = {{ amqp_de_uri }}

dewey.es.host                   = {{ data_store.elasticsearch.host }}
dewey.es.port                   = {{ data_store.elasticsearch.port }}

dewey.irods.host                = {{ irods.host }}
dewey.irods.port                = {{ irods.port }}
dewey.irods.zone                = {{ irods.zone }}
dewey.irods.user                = {{ irods.user }}
dewey.irods.password            = {{ irods.password }}
dewey.irods.default-resource    = {{ irods.default_resource }}
dewey.irods.home                = {{ irods.home }}

dewey.status.listen-port        = {{ default_service_port }}
